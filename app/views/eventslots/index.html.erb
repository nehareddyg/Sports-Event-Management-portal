<p id="notice"><%= notice %></p>

<h1>Eventslots</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @eventslots.each do |eventslot| %>
      <tr>
        <td><%= link_to 'Show', eventslot %></td>
        <td><%= link_to 'Edit', edit_eventslot_path(eventslot) %></td>
        <td><%= link_to 'Destroy', eventslot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Eventslot', new_eventslot_path %>
